{
  "name": "EMQH",
  "version": "1.1",
  "description": "A tiny quiz helper specifically for Electromagnetics courses.",
  "permissions": [
    "contextMenus",
	"browsingData",
	"tabs",
	"*://*/*"
  ],
  "browser_action": {
      "default_title": "EMQH",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "background": {  
      "scripts": ["popup.js"],    
      "persistent": true 
  },
  "icons": { "16": "icon.png" },  
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}